{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"*": [
				"types/*"
			]
		},
		"target": "es6",
		"module": "commonjs",
		"outDir": "dist",
		"sourceMap": true,
		"strict": true,
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true
	},
	"exclude": [
		"editors",
		"node_modules"
	]
}